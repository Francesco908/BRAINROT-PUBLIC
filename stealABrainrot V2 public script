-- Admin vs Admin Script by Znfpro
-- Simple Bypass Version

wait(1)

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

repeat wait() until player.Character
repeat wait() until player.Character:FindFirstChild("HumanoidRootPart")
repeat wait() until player.Character:FindFirstChild("Humanoid")

local character = player.Character
local humanoid = character.Humanoid
local rootPart = character.HumanoidRootPart

-- Variables
local basePosition = rootPart.Position
local stealActive = false
local speedBoostActive = false
local multiJumpActive = false
local originalWalkSpeed = humanoid.WalkSpeed

-- Simple bypass using old methods
local function simpleBypass()
    if speedBoostActive then
        -- Just set speed, no fancy hooks
        humanoid.WalkSpeed = 90
        humanoid.JumpPower = 90
        
        -- Keep tools equipped simply
        local speedTool = character:FindFirstChild("Speed Coil") or player.Backpack:FindFirstChild("Speed Coil")
        if speedTool and speedTool.Parent ~= character then
            humanoid:EquipTool(speedTool)
        end
    end
end

-- Speed Coil check
local function hasSpeedCoil()
    return player.Backpack:FindFirstChild("Speed Coil") or character:FindFirstChild("Speed Coil")
end

-- GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "AdminVsAdminGUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- Startup
local function showStartup()
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 350, 0, 120)
    frame.Position = UDim2.new(0.5, -175, 0.5, -60)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.BackgroundTransparency = 0.3
    frame.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = frame
    
    local label1 = Instance.new("TextLabel")
    label1.Size = UDim2.new(1, 0, 0.5, 0)
    label1.Position = UDim2.new(0, 0, 0, 0)
    label1.BackgroundTransparency = 1
    label1.Text = "Made by Znfpro"
    label1.TextColor3 = Color3.new(1, 1, 1)
    label1.TextScaled = true
    label1.Font = Enum.Font.GothamBold
    label1.Parent = frame
    
    local label2 = Instance.new("TextLabel")
    label2.Size = UDim2.new(1, 0, 0.5, 0)
    label2.Position = UDim2.new(0, 0, 0.5, 0)
    label2.BackgroundTransparency = 1
    label2.Text = "For Speed Boost you need Brainrot in hand"
    label2.TextColor3 = Color3.new(1, 1, 0)
    label2.TextScaled = true
    label2.Font = Enum.Font.Gotham
    label2.Parent = frame
    
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://76714680193450"
    sound.Volume = 0.5
    sound.Looped = true
    sound.Parent = workspace
    sound:Play()
    
    wait(4)
    frame:Destroy()
end

-- Notification
local function notify(text, color)
    local notif = Instance.new("TextLabel")
    notif.Size = UDim2.new(0, 200, 0, 40)
    notif.Position = UDim2.new(0.5, -100, 0, 50)
    notif.BackgroundColor3 = color or Color3.new(0, 0, 0)
    notif.BackgroundTransparency = 0.2
    notif.Text = text
    notif.TextColor3 = Color3.new(1, 1, 1)
    notif.TextScaled = true
    notif.Font = Enum.Font.Gotham
    notif.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = notif
    
    game:GetService("Debris"):AddItem(notif, 2)
end

-- Draggable button
local function createButton(text, pos, callback)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 150, 0, 50)
    button.Position = pos
    button.BackgroundColor3 = Color3.new(0, 0, 0)
    button.Text = text
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextScaled = true
    button.Font = Enum.Font.GothamBold
    button.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = button
    
    local dragging = false
    button.MouseButton1Down:Connect(function()
        dragging = true
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mousePos = UserInputService:GetMouseLocation()
            button.Position = UDim2.new(0, mousePos.X - 75, 0, mousePos.Y - 25)
        end
    end)
    
    button.MouseButton1Click:Connect(callback)
    return button
end

-- Speed Boost (simple version)
local function toggleSpeedBoost()
    speedBoostActive = not speedBoostActive
    
    if speedBoostActive then
        if not hasSpeedCoil() then
            notify("You need Speed Coil first!", Color3.new(1, 0, 0))
            speedBoostActive = false
            return
        end
        
        notify("Speed Boost ON", Color3.new(0, 1, 0))
        
        local speedCoil = hasSpeedCoil()
        if speedCoil and speedCoil.Parent == player.Backpack then
            humanoid:EquipTool(speedCoil)
        end
        
        -- Simple speed loop
        spawn(function()
            while speedBoostActive do
                simpleBypass()
                wait(0.1)
            end
            humanoid.WalkSpeed = originalWalkSpeed
            humanoid.JumpPower = 50
        end)
    else
        notify("Speed Boost OFF", Color3.new(1, 1, 0))
        humanoid.WalkSpeed = originalWalkSpeed
        humanoid.JumpPower = 50
    end
end

-- Steal
local function steal()
    if stealActive then return end
    stealActive = true
    
    notify("Click to set base!", Color3.new(1, 1, 0))
    
    local connection
    connection = player:GetMouse().Button1Down:Connect(function()
        basePosition = rootPart.Position
        notify("Base set! Stealing...", Color3.new(0, 1, 0))
        connection:Disconnect()
        
        wait(0.5)
        
        -- Simple backward movement
        local bodyVel = Instance.new("BodyVelocity")
        bodyVel.MaxForce = Vector3.new(4000, 0, 4000)
        bodyVel.Velocity = rootPart.CFrame.LookVector * -40
        bodyVel.Parent = rootPart
        
        wait(1)
        bodyVel:Destroy()
        
        -- Move to base
        spawn(function()
            while (rootPart.Position - basePosition).Magnitude > 8 do
                -- Jump
                local jumpVel = Instance.new("BodyVelocity")
                jumpVel.MaxForce = Vector3.new(0, 4000, 0)
                jumpVel.Velocity = Vector3.new(0, 100, 0)
                jumpVel.Parent = rootPart
                
                wait(0.4)
                jumpVel:Destroy()
                
                -- Walk
                local direction = (basePosition - rootPart.Position).Unit
                local walkVel = Instance.new("BodyVelocity")
                walkVel.MaxForce = Vector3.new(4000, 0, 4000)
                walkVel.Velocity = Vector3.new(direction.X * 20, 0, direction.Z * 20)
                walkVel.Parent = rootPart
                
                wait(1.5)
                walkVel:Destroy()
                wait(0.3)
            end
            
            notify("Steal Complete!", Color3.new(0, 1, 0))
            stealActive = false
        end)
    end)
end

-- Multi Jump
local function toggleMultiJump()
    multiJumpActive = not multiJumpActive
    
    if multiJumpActive then
        notify("Multi Jump ON", Color3.new(0, 1, 0))
        
        local jumpConnection
        jumpConnection = UserInputService.JumpRequest:Connect(function()
            if multiJumpActive then
                local jumpVel = Instance.new("BodyVelocity")
                jumpVel.MaxForce = Vector3.new(0, 4000, 0)
                jumpVel.Velocity = Vector3.new(0, 80, 0)
                jumpVel.Parent = rootPart
                
                wait(0.2)
                jumpVel:Destroy()
            else
                jumpConnection:Disconnect()
            end
        end)
    else
        notify("Multi Jump OFF", Color3.new(1, 1, 0))
    end
end

-- Initialize
showStartup()

local stealButton = createButton("Steal", UDim2.new(0, 50, 0, 200), steal)
local speedButton = createButton("Speed Boost", UDim2.new(0, 220, 0, 200), toggleSpeedBoost)
local jumpButton = createButton("Multi Jump", UDim2.new(0, 390, 0, 200), toggleMultiJump)

-- Button colors
spawn(function()
    while wait(0.1) do
        speedButton.BackgroundColor3 = speedBoostActive and Color3.new(0, 0.5, 0) or Color3.new(0, 0, 0)
        jumpButton.BackgroundColor3 = multiJumpActive and Color3.new(0, 0.5, 0) or Color3.new(0, 0, 0)
    end
end)

-- Respawn handling
player.CharacterAdded:Connect(function(newChar)
    repeat wait() until newChar:FindFirstChild("HumanoidRootPart")
    character = newChar
    humanoid = character.Humanoid
    rootPart = character.HumanoidRootPart
    originalWalkSpeed = humanoid.WalkSpeed
    stealActive = false
    speedBoostActive = false
    multiJumpActive = false
end)
